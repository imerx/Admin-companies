
<div id="order-modal" class="modal fade">
  <div class="modal-dialog">
    <div class="modal-content">
      
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>
        </button>
          <h4 class="modal-title"><%= title %></h4>
      </div>
      <!-- start rails code -->
     
        <div class="modal-body">
          <!-- search form -->
          <div class="alert alert-danger" role="alert" style="display: none;"></div>
          


         <%= form_for(@order, remote: true, :html => { :role => "form" }) do |f| %>

              
          <div id="search-form" style="display:none;"></div>

          
          <div class="form-group">
            <%= f.text_field :user_id,:value => params['1'], class: "form-control input-lg" %>
          </div>
          
          <div class="form-group">
            <%= f.text_field :check_out_at, class: "form-control input-lg", placeholder: "check_out_at" %>
          </div>
           <div class="form-group">
            <%= f.text_field :total_price, class: "form-control input-lg", placeholder: "Precio" %>
          </div>
        
        
        </div><!-- /.modal-body -->
                
        <div class="modal-footer">
             <%= f.submit  class: "btn btn-primary btn-lg btn-block" %>
        </div>
       <% end %>

    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->






